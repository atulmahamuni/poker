{
  "name": "poker",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "node app.js",
    "test": "jest --testPathPattern='__tests__/*'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/atulmahamuni/poker"
  },
  "dependencies": {
    "express": "~3.4.7",
    "jade": "~1.7.0",
    "less": "~2.0.0",
    "less-middleware": "~1.0.4",
    "socket.io": "~1.2.0",
    "connect": "~3.3.3"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/__tests__/utils.test.js"
    ],
    "reporters": [
      "default"
    ],
    "testPathIgnorePatterns": [
      "__tests__/utils.test.js"
    ],
    "verbose": true
  },
  "jest-junit": {
    "outputDirectory": "target/jest-junit"
  }
}
